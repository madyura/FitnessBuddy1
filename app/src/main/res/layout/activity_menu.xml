<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu">

    <TextView
        android:id="@+id/textViewFitnessBuddyId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/app_name"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />

    <ScrollView
        android:id="@+id/scrollViewMenuId"
        android:layout_width="409dp"
        android:layout_height="676dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline10">

        <LinearLayout
            android:id="@+id/linearLayoutMenuId"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textViewBreakfastId"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text="@string/breakfast" />

            <LinearLayout
                android:id="@+id/linearLayoutBreakfastDescriptionId"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textViewOatmealId"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:text="@string/oatmeal" />

                <TextView
                    android:id="@+id/textViewMilkId"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:text="@string/milk" />

                <TextView
                    android:id="@+id/textViewDriedFruitsId"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:text="@string/dried_fruits" />

                <TextView
                    android:id="@+id/textViewEggsId"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:text="@string/eggs" />
            </LinearLayout>

            <TextView
                android:id="@+id/textViewFirstSnackId"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text="@string/first_snack" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textViewDriedRiceBreadId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/dried_rice" />

                <TextView
                    android:id="@+id/textViewHamId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/ham" />

                <TextView
                    android:id="@+id/textViewPickleId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/pickle" />
            </LinearLayout>

            <TextView
                android:id="@+id/textViewLunchId"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text="@string/lunch" />

            <LinearLayout
                android:id="@+id/LinearLayoutLunchId"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textViewBulgurId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/bulgur" />

                <TextView
                    android:id="@+id/textViewChickenBreastId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/chicken_breast" />

                <TextView
                    android:id="@+id/textViewSaladId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/salad" />
            </LinearLayout>

            <TextView
                android:id="@+id/textViewSecondSnackId"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text="@string/second_snack" />

            <LinearLayout
                android:id="@+id/LinearLayoutSecondSnackId"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textViewQuarkId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/quark" />

                <TextView
                    android:id="@+id/textViewAppleId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/apple" />
            </LinearLayout>

            <TextView
                android:id="@+id/textViewDinnerId"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text="@string/dinner" />

            <LinearLayout
                android:id="@+id/LinearLayoutDinnerId"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textViewFishId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/fish" />

                <TextView
                    android:id="@+id/textViewRiceId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/rice" />

                <TextView
                    android:id="@+id/textViewSalad2Id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/salad" />
            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>